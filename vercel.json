{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/predict.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/assess.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/batch_predict.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "api/index.py"
    },
    {
      "src": "/api/predict",
      "dest": "api/predict.py"
    },
    {
      "src": "/api/assess",
      "dest": "api/assess.py"
    },
    {
      "src": "/api/batch_predict",
      "dest": "api/batch_predict.py"
    }
  ]
}
